// 3: const bool = true;
// a) the constant boolean will always be true
const answer3b = undefined; 

// 4: const x = 10 - 2;
// a) the constant x will point to the mathamatical problem of 10 minus 2
const answer4b = undefined; 

// 5: 'sub' + 'station';
// a) This will put the two words together to make one
const answer5b = 'substation';

// 6: console.log('Hello world');
// a) This will be logged in a debugging console.
const answer6b = 'Helloword';

// 7: true || false;
// a) there is one true so this will come out true
const answer7b = true;

// 8: null;
// a) there is no value
const answer8b = null;

// 9: console.log(4*5);
// a)message will be logged onto a debugging console.
const answer9b = 20;

// 10: const z = Math.round(3.2);
// a)expected output is 3
const answer10b = 'z = 3'

